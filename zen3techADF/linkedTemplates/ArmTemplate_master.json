{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "zen3techADF"
		},
		"LS_ADLSgen2_TGT_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_ADLSgen2_TGT'"
		},
		"LS_ORC_SRC_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_ORC_SRC'"
		},
		"LS_ADLSgen2_TGT_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://zen3techsa.dfs.core.windows.net"
		},
		"LS_AzureKV_managedIdentity_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://Zen3techHiddenKV.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "zen3techADF_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_ADLSgen2_TGT_accountKey": {
						"value": "[parameters('LS_ADLSgen2_TGT_accountKey')]"
					},
					"LS_ORC_SRC_connectionString": {
						"value": "[parameters('LS_ORC_SRC_connectionString')]"
					},
					"LS_ADLSgen2_TGT_properties_typeProperties_url": {
						"value": "[parameters('LS_ADLSgen2_TGT_properties_typeProperties_url')]"
					},
					"LS_AzureKV_managedIdentity_properties_typeProperties_baseUrl": {
						"value": "[parameters('LS_AzureKV_managedIdentity_properties_typeProperties_baseUrl')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}